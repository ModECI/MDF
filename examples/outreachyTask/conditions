	node [color="#444444" fontcolor="#444444" penwidth=1 shape=box style=rounded]
	B [label=<<table border="0" cellborder="0"><tr><td colspan="2"><b>B</b></td></tr><tr><td><font color="#1666ff">subtractor</font> = 3.0</td></tr><tr><td><b><font color="#1666ff">result</font></b> = <font color="#1666ff">result</font> - <font color="#1666ff">subtractor</font></td></tr><tr><td><font color="#ffa1d">condition</font> = <b>B</b> will run every <b>2</b> calls of <b>A</b></td></tr><tr><td><font color="#cc3355">output</font> = <font color="#1666ff">result</font> </td></tr></table>>]
