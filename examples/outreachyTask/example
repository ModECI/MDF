	node1 -> node2 [label=<edge_node1_node2 (<font color="#cc3355">output</font> -&gt; <font color="#188855">input</font>)> arrowhead=empty]
	node2 -> node3 [label=<edge_node2_node3 (<font color="#cc3355">output</font> -&gt; <font color="#188855">input</font>)> arrowhead=empty]
