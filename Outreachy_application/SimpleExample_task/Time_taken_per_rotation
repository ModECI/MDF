digraph Graph_for_time_taken_per_rotation {
	graph [rankdir=LR]
	node [color="#00b233" fillcolor="#00b233" fontcolor=black penwidth=2 shape=box style="rounded,filled"]
	Rotation_node [label=<<table border="0" cellborder="0"><tr><td colspan="2"><b>Rotation_node</b></td></tr><tr><td><i>color = 0 .7 .2</i></td></tr><tr><td><font color="#1666ff">number_of_rotation_per_turn</font> = 1.0</td></tr><tr><td><b><font color="#1666ff">number_of_rotations</font></b> = <font color="#1666ff">number_of_rotations</font> + <font color="#1666ff">number_of_rotation_per_turn</font></td></tr><tr><td><font color="#ffa1d">condition</font> = <b>Rotation_node</b> will <b>Always</b> run</td></tr><tr><td><font color="#cc3355">number_of_rotations</font> = <font color="#1666ff">number_of_rotations</font> </td></tr></table>>]
	node [color="#007f1fe" fillcolor="#007f1fe" fontcolor=black penwidth=2 shape=box style="rounded,filled"]
	Distance_node [label=<<table border="0" cellborder="0"><tr><td colspan="2"><b>Distance_node</b></td></tr><tr><td><i>color = 0 .5 2</i></td></tr><tr><td><font color="#1666ff">distance_per_rotation</font> = 1000</td></tr><tr><td><b><font color="#1666ff">distance</font></b> = <font color="#1666ff">distance</font> + <font color="#1666ff">distance_per_rotation</font></td></tr><tr><td><font color="#ffa1d">condition</font> = <b>Distance_node</b> will run every <b>3</b> calls of <b>Rotation_node</b></td></tr><tr><td><font color="#cc3355">distance</font> = <font color="#1666ff">distance</font> </td></tr></table>>]
	node [color="#003333" fillcolor="#003333" fontcolor=white penwidth=2 shape=box style="rounded,filled"]
	time_node [label=<<table border="0" cellborder="0"><tr><td colspan="2"><b>time_node</b></td></tr><tr><td><i>color = 0 .2 .2</i></td></tr><tr><td><b><font color="#1666ff">time_taken_per_rotation</font></b> = <font color="#1666ff">time_taken_per_rotation</font> + 1</td></tr><tr><td><font color="#ffa1d">condition</font> = <b>time_node</b> will run every <b>3</b> calls of <b>Rotation_node</b></td></tr><tr><td><font color="#cc3355">time_taken_per_rotation</font> = <font color="#1666ff">time_taken_per_rotation</font> </td></tr></table>>]
}
